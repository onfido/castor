// theme generation by Theo (https://github.com/salesforce-ux/theo)
// edit `$$THEME.json` instead

@use '../theme' as castor;

@mixin $$THEME_NAME$$($types...) {
  @include castor.theme('$$THEME_NAME$$', $types...) {
    color-scheme: $$COLOR_SCHEME$$;

  {{#each props as |prop|}}
    --ods-{{prop.category}}-{{prop.name}}: {{prop.value}};
  {{/each}}
  }
}
